pragma solidity ^0.8.13;

import "forge-std/Test.sol";
import "../src/OnChainMetadata.sol";
import "../src/StorageMetadata.sol";

contract ComparisonTest is Test {
    StorageMetadata public storageMetadata = new StorageMetadata();
    OnChainMetadata public onChainMetadata = new OnChainMetadata();

    function setUp() public {
    }

    function testSmallData() public {
        storageMetadata.setMediaData(
           address(this),
           "testing description",
            "this is quote a small string",
            ""
        );
        onChainMetadata.setMediaData(
           address(this),
           "testing description",
            "this is quote a small string",
            ""
        );
    }

    function testMediumData() public {
        storageMetadata.setMediaData(
           address(this),
            "this is quote a small string",
            "data:image/svg+xml;base64,",
            "data:image/svg+xml;base64,"
        );
        onChainMetadata.setMediaData(
           address(this),
            "this is quote a small string",
            "data:image/svg+xml;base64,",
            "data:image/svg+xml;base64,"
        );
    }

}
